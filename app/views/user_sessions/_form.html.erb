<div class="user-form">
  <%= form_with url: login_path, method: :post do |f| %>
    <div class="field">
      <%= f.label :user_name, 'ユーザーネーム' %><br>
      <%= f.text_field :user_name %>
    </div>
    <div class="field">
      <%= f.label :password, 'パスワード' %><br>
      <%= f.password_field :password %>
    </div>
    <div class="actions">
      <%= f.submit "ログイン", class: 'btn btn-index' %>
    </div>
  <% end %>
</div>
